{
    "name": "codemind-ai",
    "displayName": "CodeMind AI - Next-Gen Coding Agent",
    "description": "ðŸ¤– AI coding agent with 6 specialized agents, 10+ models (Groq, DeepSeek, Gemini), deep codebase intelligence, and complete safety system. Free tier focused!",
    "version": "1.0.0",
    "publisher": "codemind",
    "author": {
        "name": "CodeMind Team",
        "email": "support@codemind.dev"
    },
    "license": "MIT",
    "icon": "resources/icon.png",
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Muzammil0001/code-agent.git"
    },
    "bugs": {
        "url": "https://github.com/Muzammil0001/code-agent/issues"
    },
    "homepage": "https://github.com/Muzammil0001/code-agent#readme",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages",
        "Machine Learning",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "ai",
        "coding-assistant",
        "code-generation",
        "refactoring",
        "autocomplete",
        "llm",
        "free-ai"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onCommand:codemind.openPanel",
        "onCommand:codemind.generateCode",
        "onCommand:codemind.refactorCode",
        "onCommand:codemind.explainCode",
        "onCommand:codemind.generateTests",
        "onCommand:codemind.generateDocs",
        "onCommand:codemind.imageToCode"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "codemind.openPanel",
                "title": "Open CodeMind AI Panel",
                "category": "CodeMind"
            },
            {
                "command": "codemind.generateCode",
                "title": "Generate Code",
                "category": "CodeMind"
            },
            {
                "command": "codemind.refactorCode",
                "title": "Refactor Code",
                "category": "CodeMind"
            },
            {
                "command": "codemind.explainCode",
                "title": "Explain Code",
                "category": "CodeMind"
            },
            {
                "command": "codemind.generateTests",
                "title": "Generate Tests",
                "category": "CodeMind"
            },
            {
                "command": "codemind.generateDocs",
                "title": "Generate Documentation",
                "category": "CodeMind"
            },
            {
                "command": "codemind.imageToCode",
                "title": "Convert Image to Code",
                "category": "CodeMind"
            },
            {
                "command": "codemind.analyzeCodebase",
                "title": "Analyze Codebase",
                "category": "CodeMind"
            },
            {
                "command": "codemind.clearPermissions",
                "title": "Clear Permission Memory",
                "category": "CodeMind"
            },
            {
                "command": "codemind.toggleTurboMode",
                "title": "Toggle Turbo Mode",
                "category": "CodeMind"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "codemind-sidebar",
                    "title": "CodeMind AI",
                    "icon": "resources/sidebar-icon.svg"
                }
            ]
        },
        "views": {
            "codemind-sidebar": [
                {
                    "type": "webview",
                    "id": "codemind.panel",
                    "name": "Dashboard"
                }
            ]
        },
        "configuration": {
            "title": "CodeMind AI",
            "properties": {
                "codemind.primaryModel": {
                    "type": "string",
                    "default": "groq-llama-3.1-70b",
                    "enum": [
                        "groq-llama-3.1-70b",
                        "groq-mixtral-8x7b",
                        "groq-llama-3.1-8b",
                        "deepseek-coder",
                        "deepseek-chat",
                        "gemini-pro",
                        "openai-gpt-4o-mini",
                        "claude-haiku",
                        "ollama-local",
                        "lmstudio-local"
                    ],
                    "description": "Primary AI model to use for code generation"
                },
                "codemind.enableAutoFallback": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically fallback to alternative models if primary fails"
                },
                "codemind.enableLocalModels": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable local LLM support (Ollama, LM Studio)"
                },
                "codemind.ollamaUrl": {
                    "type": "string",
                    "default": "http://localhost:11434",
                    "description": "Ollama server URL"
                },
                "codemind.lmstudioUrl": {
                    "type": "string",
                    "default": "http://localhost:1234",
                    "description": "LM Studio server URL"
                },
                "codemind.groqApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Groq API key (free tier available)"
                },
                "codemind.deepseekApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "DeepSeek API key (free tier available)"
                },
                "codemind.geminiApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Google Gemini API key (free tier available)"
                },
                "codemind.openaiApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "OpenAI API key (for free tier models)"
                },
                "codemind.anthropicApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Anthropic API key (for Claude Haiku free tier)"
                },
                "codemind.enableInlineSuggestions": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable real-time inline code suggestions"
                },
                "codemind.enableCodebaseAnalysis": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable deep codebase intelligence and analysis"
                },
                "codemind.turboMode": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable Turbo Mode for faster code generation (may reduce quality)"
                },
                "codemind.safetyLevel": {
                    "type": "string",
                    "default": "strict",
                    "enum": [
                        "strict",
                        "moderate",
                        "relaxed"
                    ],
                    "description": "Safety level for permission checks"
                },
                "codemind.theme": {
                    "type": "string",
                    "default": "auto",
                    "enum": [
                        "auto",
                        "dark",
                        "light",
                        "custom"
                    ],
                    "description": "UI theme for CodeMind panel"
                },
                "codemind.maxContextLines": {
                    "type": "number",
                    "default": 1000,
                    "description": "Maximum lines of code to include in context"
                },
                "codemind.enableParallelExecution": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable parallel task execution for better performance"
                },
                "codemind.cacheEmbeddings": {
                    "type": "boolean",
                    "default": true,
                    "description": "Cache code embeddings for faster retrieval"
                }
            }
        },
        "keybindings": [
            {
                "command": "codemind.openPanel",
                "key": "ctrl+shift+m",
                "mac": "cmd+shift+m"
            },
            {
                "command": "codemind.generateCode",
                "key": "ctrl+shift+g",
                "mac": "cmd+shift+g"
            },
            {
                "command": "codemind.explainCode",
                "key": "ctrl+shift+e",
                "mac": "cmd+shift+e",
                "when": "editorHasSelection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run build:extension",
        "build": "npm run build:extension && npm run build:webview",
        "build:extension": "webpack --mode production --config webpack.extension.config.js",
        "build:webview": "cd webview && npm run build",
        "watch": "concurrently \"npm run watch:extension\" \"npm run watch:webview\"",
        "watch:extension": "webpack --mode development --watch --config webpack.extension.config.js",
        "watch:webview": "cd webview && npm run dev",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "test:unit": "jest",
        "test:integration": "jest --config jest.integration.config.js",
        "package": "vsce package",
        "install:webview": "cd webview && npm install"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/vscode": "^1.85.0",
        "@types/jest": "^29.5.11",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vscode/test-electron": "^2.3.8",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "@vscode/vsce": "^2.22.0"
    },
    "dependencies": {
        "axios": "^1.6.2",
        "tree-sitter": "^0.21.0",
        "tree-sitter-typescript": "^0.20.5",
        "tree-sitter-javascript": "^0.21.2",
        "tree-sitter-python": "^0.20.4",
        "tree-sitter-go": "^0.20.0",
        "tree-sitter-rust": "^0.21.0",
        "tree-sitter-java": "^0.20.2",
        "better-sqlite3": "^9.2.2",
        "openai": "^4.24.1",
        "@anthropic-ai/sdk": "^0.10.2",
        "@google/generative-ai": "^0.1.3",
        "groq-sdk": "^0.3.0",
        "diff": "^5.1.0",
        "fast-glob": "^3.3.2",
        "ignore": "^5.3.0",
        "p-queue": "^8.0.1",
        "zod": "^3.22.4"
    }
}